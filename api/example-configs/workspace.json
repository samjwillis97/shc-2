{
  "name": "example-workspace-config",
  "imports": ["./variables.json", "./variableGroups.json"],
  "plugins": ["shc-plugin-example"],
  "pluginConfig": {
    "base": {
      "test-config-value": "Hello, World! - {{environment.api-gw-url}}",
      "date": "{{date}}"
    }
  },
  "variables": {
    "url": "{{environment.api-gw-url}}",
    "date": "26/08/2024"
  },
  "hooks": {
    "pre-request": ["example.auth", "base.logContext", "base.logConfig"],
    "post-request": ["base.logContext"]
  },
  "headers": {
    "Content-Type": "application/json"
  },
  "endpoints": {
    "getSomething": {
      "method": "GET",
      "endpoint": "{{url}}/get",
      "headers": {
        "endpoint-header": "should-be-there",
        "user": "{{user.email}}"
      },
      "variables": {
        "date": "27/08/2024"
      }
    },
    "endpointTheSecond": {
      "method": "GET",
      "endpoint": "{{url}}/get"
    }
  }
}
