{
  "name": "example-workspace-config",
  "imports": ["./variables.json"],
  "plugins": [
    "url-builder@file:/Users/samwillis/code/github.com/samjwillis97/shc-2/plugins/url-builder",
    "my-auth@file:/Users/samwillis/code/github.com/samjwillis97/shc-2/plugins/my-auth"
  ],
  "variables": {
    "baseUrl": "{{url-builder.build}}"
  },
  "variableGroups": {
    "user": {
      "name": "test user",
      "email": "test@tester.com"
    },
    "secondUser": {
      "name": "Second user",
      "email": "second@tester.com"
    }
  },
  "hooks": {
    "pre-request": ["my-auth.authenticate"]
  },
  "endpoints": {
    "getSomething": {
      "method": "GET",
      "endpoint": "{{baseUrl}}/something"
    }
  }
}
